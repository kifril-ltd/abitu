{% extends 'base.html.twig' %}

{% block title %}StudentResult index{% endblock %}

{% block body %}
    <h1>StudentResult index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Result</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for student_result in student_results %}
            <tr>
                <td>{{ student_result.id }}</td>

                <td>
                    <a href="{{ path('app_student_result_show', {'id': student_result.id}) }}">show</a>
                    <a href="{{ path('app_student_result_edit', {'id': student_result.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_student_result_new') }}">Create new</a>
{% endblock %}
